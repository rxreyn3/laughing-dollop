# Example channel configuration
# Copy this file to channels.yaml and update with your channels
channels:
  - id: "CHANNEL_ID_1"
    name: "support-general"
    description: "General support channel"
    enabled: true

  - id: "CHANNEL_ID_2"
    name: "support-technical"
    description: "Technical support discussions"
    enabled: true

  - id: "CHANNEL_ID_3"
    name: "support-design"
    description: "Design and UX support"
    enabled: false  # Disabled channels won't be monitored
